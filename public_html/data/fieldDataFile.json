{
    "Name": {
        "RegexMatch": "^(?<first>[A-Za-z-]+) (?<last>[A-Za-z- ]*)$",
        "DivisibleFields": {
            "FirstName": "first",
            "LastName": "last"
        },
        "Display": {
            "English": "Name"
        }
    },

    "FirstName": {
        "Display": {
            "English": "First Name"
        }
    },

    "LastName": {
        "Display": {
            "English": "Last Name"
        }
    },

    "Address": {
        "RegexMatch": "^[0-9]+ [\\S\\s]*$",
        "Display": {
            "English": "Street Address"
        }
    },

    "FullAddress": {
        "Display": {
            "English": "Full Address"
        },
        "CombinedFormatting": {
            "FormattedString": "<Address>, <City> <PostalCode>",
            "FieldsToInclude": ["City", "Address", "PostalCode"]
        }
    },

    "Province": {
        "Display": {
            "English": "Province"
        }
    },

    "Apartment": {
        "Display": {
            "English": "Apartment"
        }
    },

    "City": {
        "RegexMatch": "^(?![ -])([-]?[a-zA-Z])+$",
        "Display": {
            "English": "City"
        }
    },

    "PostalCode": {
        "RegexMatch": "^[A-z][0-9][A-z] ?[0-9][A-z][0-9]$",
        "Display": {
            "English": "Postal Code"
        },
        "FieldValueModifications": {
            "Formatted": []
        },
        "Autofill": [["Address", "City"]]
    },

    "Email": {
        "RegexMatch": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$",
        "Display": {
            "English": "Email"
        }
    },

    "Phone": {
        "RegexMatch": "^\\(?[0-9]{3}\\)? ?-?[0-9]{3} ?-?[0-9]{4}$",
        "FieldValueModifications": {
            "OnlyNumbers": ["\\(|\\)|-| ", ""],
            "Formatted": ["\\(?([0-9]{3})[\\) -]*([0-9]{3})[\\) -]*([0-9]{4})", "($1)$2-$3"]
        },
        "Display": {
            "English": "Phone"
        }
    },

    "DOB": {
        "RegexMatch": "^(?<year>\\d{2,4})-(?<month>\\d{2})-(?<day>\\d{2})$",
        "FieldValueModifications": {
            "ShortYear": [],
            "Spaced": ["-", "  "]
        },
        "DivisibleFields": {
            "DOBY": "year",
            "DOBM": "month",
            "DOBD": "day"
        },
        "Display": {
            "English": "Date of Birth"
        }
    },

    "DOBY": {
        "RegexMatch": "^\\d{4}$",
        "FieldValueModifications": {
            "TwoDigit": ["^([0-9]{2})?([0-9]{2})$", "$2"]
        },
        "Display": {
            "English": "Date of Birth (Year)"
        }
    },

    "DOBM": {
        "RegexMatch": "January|February|March|April|May|June|July|August|September|October|November|December",
        "FieldValueModifications": {
            "Numeric": []
        },
        "Display": {
            "English": "Date of Birth (Month)"
        }
    },

    "DOBD": {
        "Display": {
            "English": "Date of Birth (Day)"
        }
    },

    "Prereq1Name": {
        "Display": {
            "English": "Prerequisite 1 Name"
        }
    },

    "Prereq1Date": {
        "Display": {
            "English": "Prerequisite 1 Date"
        }
    },
    "Prereq1Location": {
        "Display": {
            "English": "Prerequisite 1 Location"
        }
    },

    "Prereq2Name": {
        "Display": {
            "English": "Prerequisite 2 Name"
        }
    },

    "Prereq2Date": {
        "Display": {
            "English": "Prerequisite 2 Date"
        }
    },
    "Prereq2Location": {
        "Display": {
            "English": "Prerequisite 2 Location"
        }
    },

    "LSSId": {
        "Display": {
            "English": "LSS ID"
        }
    }
}